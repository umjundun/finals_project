<div class="subsection-title">Meet our Devs</div>
<div class="section-container">
  <div class="row" >
    <% @users.each do |user| %>
      <% if user.organization_id == 1 %>
        <div class="card-project mb-3 mr-1 ml-1">
          <img class="avatar-large" alt="avatar-large" src="https://kitt.lewagon.com/placeholder/users/arthur-littm" />
          <div class="card-project-infos">
            <h2><%= link_to user.first_name, user_path(user) %></h2>
          </div>
        </div>
      <% end %>
    <% end %>
  </div>
  <%= pie_chart User.group(:group).count %>
  <%#= line_chart User.group_by_day(:created_at).count %>
  <%#= bar_chart @users.user_chart %>
</div>

<%= link_to user_path(user) do %>
  <div class="card-dev">
    <!-- Card image -->
    <% cl_image_tag user.photo %>
    <%= image_tag 'https://images.unsplash.com/photo-1518822275865-16eec4d3023d?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1050&q=80' %>
    <!-- Card content -->
    <div class="card-dev-content">
      <!-- Title -->
      <h3><%= user.full_name %></h3>
    </div>
  </div>
<% end %>
